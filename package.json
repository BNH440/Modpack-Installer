{
  "name": "modpack-installer",
  "version": "0.2.4",
  "description": "An Installer for Modpacks",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "mac": "build --mac",
    "linux": "build --linux",
    "win": "build --win",
    "clean": "rm -rf dist",
    "publish": "build --win -p always"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BNH440/Modpack-Installer.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Blake Haug",
  "license": "None",
  "devDependencies": {
    "electron": "^10.1.3",
    "electron-builder": "^10.17.3",
    "electron-packager": "^8.7.2"
  },
  "dependencies": {
    "edit-json-file": "^1.4.1",
    "electron-sudo": "^4.0.12",
    "electron-updater": "^4.3.5",
    "jquery": "^3.5.1",
    "request": "^2.88.2",
    "unzipper": "^0.10.11"
  },
  "build": {
    "productName": "Modpack Installer",
    "appId": "tech.blakeh.modpackInstaller",
    "win": {
      "target": "nsis",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "nsis": {
      "guid": "eb1a0fbb-fc70-428e-97f1-fa7080894806",
      "oneClick": false,
      "perMachine": true
    },
    "publish": [
      {
        "provider": "github",
        "owner": "BNH440",
        "repo": "Modpack-Installer"
      }
    ]
  },
  "bugs": {
    "url": "https://github.com/electron/electron-quick-start/issues"
  },
  "homepage": "https://github.com/electron/electron-quick-start#readme"
}
